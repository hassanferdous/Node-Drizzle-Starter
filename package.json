{
	"name": "express-api-v1",
	"version": "1.0.0",
	"main": "dist/server.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "NODE_ENV=production node dist/src/server.js",
		"dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
		"generate:api": "node src/scripts/generate-api.js",
		"seed-db": "ts-node src/seed.ts",
		"install:container": "docker exec -it drizzle_express pnpm install"
	},
	"dependencies": {
		"bcryptjs": "^3.0.2",
		"bullmq": "^5.56.5",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"drizzle-orm": "^0.44.2",
		"ejs": "^3.1.10",
		"express": "^5.1.0",
		"express-rate-limit": "^8.0.1",
		"ioredis": "^5.6.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"nodemailer": "^7.0.5",
		"passport": "^0.7.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-local": "^1.0.0",
		"pg": "^8.16.3",
		"pnpm": "^10.13.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"uuid": "^11.1.0",
		"winston": "^3.18.3",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/dotenv": "^8.2.3",
		"@types/ejs": "^3.1.5",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/node": "^18.19.113",
		"@types/nodemailer": "^6.4.17",
		"@types/passport": "^1.0.17",
		"@types/passport-google-oauth20": "^2.0.16",
		"@types/passport-local": "^1.0.38",
		"@types/pg": "^8.15.4",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"concurrently": "^9.2.0",
		"drizzle-kit": "^0.31.4",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3"
	}
}
